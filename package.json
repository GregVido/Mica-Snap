    "electron": "21.1.0"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/GregVido/Mica-Snap.git"
  },
  "build": {
    "appId": "com.mica.MicaSnap",
    "win": {
      "asar": true,
      "target": "nsis",
      "icon": "icon/app.ico"
    }
  }
