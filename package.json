{
  "name": "micasnap",
  "version": "1.0.2",
  "description": "Snapchat app with mica effect.",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "rebuild": "node_modules\\.bin\\electron-rebuild",
